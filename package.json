{
  "name": "fz-browse",
  "version": "0.0.0",
  "scripts": {
    "dev": "node server.mjs",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr app/entry-server.tsx --outDir dist/server",
    "generate": "vite build --outDir dist/static && npm run build:server",
    "serve": "cross-env NODE_ENV=production node server.mjs",
    "debug": "node --inspect-brk server",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepare": "git config --local core.hooksPath .githooks"
  },
  "dependencies": {
    "@github/mini-throttle": "^2.1.0",
    "ansi-to-react": "^6.1.6",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "execa": "^6.1.0",
    "meow": "^10.1.2",
    "open": "^8.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-highlight-words": "^0.18.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "web-streams-polyfill": "^3.2.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.3",
    "@types/react-dom": "^18.0.0",
    "@types/react-highlight-words": "^0.16.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/serve-static": "^1.13.10",
    "@vitejs/plugin-react": "^1.3.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "lint-staged": "^12.3.8",
    "prettier": "^2.6.2",
    "serve-static": "^1.14.1",
    "typescript": "^4.6.3",
    "vite": "^2.8.6",
    "vitest": "^0.8.0"
  },
  "resolutions": {
    "@types/react": "^18.0.3",
    "@types/react-dom": "^18.0.0"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  }
}
