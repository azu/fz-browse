{
  "name": "fz-browse",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "node server.mjs",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr app/entry-server.tsx --outDir dist/server",
    "generate": "vite build --outDir dist/static && npm run build:server && node prerender.mjs",
    "serve": "cross-env NODE_ENV=production node server.mjs",
    "debug": "node --inspect-brk server",
    "type-check": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@github/mini-throttle": "^2.1.0",
    "ansi-to-react": "^6.1.6",
    "execa": "^6.1.0",
    "meow": "^10.1.2",
    "open": "^8.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-highlight-words": "^0.17.0",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "web-streams-polyfill": "^3.2.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-highlight-words": "^0.16.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/serve-static": "^1.13.10",
    "@vitejs/plugin-react": "^1.2.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "serve-static": "^1.14.1",
    "typescript": "^4.6.3",
    "vite": "^2.8.6",
    "vitest": "^0.8.0"
  }
}
